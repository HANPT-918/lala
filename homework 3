import java.util.Scanner;
public class study {
    public static void main(String[] args){
        Scanner input = new Scanner(System.in);
        int hour,mintute;
        hour= input.nextInt();
        mintute= input.nextInt();
        int durationH,durationM;
        if(hour<=13){
            durationH=12-hour;
            durationM=60-mintute;
        }
        else{
            durationH=24-hour+13-1;
            durationM=60-mintute;
        }
        if(durationH==0)
            System.out.println(durationM);
        else if(durationM==60){
            System.out.println(durationH+1);
        }
        else{
            System.out.print(durationH);
            System.out.println(" "+durationM);
        }
    }
}
